#!/bin/bash
#Generated by JetBrains Toolbox 1.28.1.15219 at 2024-03-03T14:09:11.860779

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/bbos/Library/Application Support/JetBrains/Toolbox/apps/WebStorm/ch-0/233.11799.293/WebStorm.app/Contents/MacOS/webstorm" $wait --args "${ideargs[@]}"